{% extends 'base.html.twig' %}

{% block title %}Hello ConferenceController!{% endblock %}

{% block body %}
    <h2>Give your feedback!</h2>

    {% for conference in conferences %}
        <h4>
            
            <a href="{{ path('conference', { id : conference.id } ) }}">{{ conference }}</a>
        </h4>
    {% endfor %}
{% endblock %}
